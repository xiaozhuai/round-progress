<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:round_progress="http://schemas.android.com/apk/res/io.github.xiaozhuai.roundprogressbar"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

	<io.github.xiaozhuai.roundprogressbar.RoundProgressBar 
	    android:id="@+id/progress"
	    android:clickable="true"
	    android:layout_width="92dp"
	    android:layout_height="92dp"
	    android:layout_gravity="center"
	    round_progress:roundForegroundColor="#FFFEDB3E"
	    round_progress:roundBackgroundColor="#60FEDB3E"
	    round_progress:roundWidth="14dp"
	    round_progress:zeroPointRoundSize="10dp"
	    round_progress:zeroPointRoundColor="#FFFFFFFF"
	    round_progress:percent="0.64" />

</FrameLayout>
